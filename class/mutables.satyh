@import: table-of-contents

module Mutables : sig

    val num-figure : int ref
    val num-chapter : int ref
    val num-section : int ref
    val num-subsection : int ref
    val num-theorems : int ref

    val toc-acc-ref : (toc-element list) ref

end = struct

    let-mutable num-figure <- 0
    let-mutable num-chapter <- 0
    let-mutable num-section <- 0
    let-mutable num-subsection <- 0
    let-mutable num-theorems <- 0

    let-mutable toc-acc-ref <- []

end